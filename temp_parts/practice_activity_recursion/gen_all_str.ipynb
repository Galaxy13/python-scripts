    "def gen_all_str(word):\n",
    "    temp_list = ['']\n",
    "    if len(word) == 1:\n",
    "        return word\n",
    "    else:\n",
    "        for elem in temp_list:\n",
    "            temp_list.append(elem+word[0])\n",
    "        return gen_all_str(word[1:])\n",
    "\n",
    "gen_all_str('abc')"
   ]
